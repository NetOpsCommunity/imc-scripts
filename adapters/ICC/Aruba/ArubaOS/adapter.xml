
<adapter name="ArubaOS">
	<description>ArubaOS Controllers</description>
	<version>1.0.0</version>
	<services>
		<service name="CLICommon">
			<item type="common">ArubaOS_Common_CLI.xml</item>
		</service>

		<service name="CleanupParser">
			<item type="parser_definition">ArubaOS_Cleanup_Parser.xml</item>
			<item type="perl_script">ArubaOS_Cleanup_Parser_Script.pl</item>
		</service>

		<service name="ConfigBackup">
			<item type="builder_definition">ArubaOS_Config_Backup_Builder.xml</item>
			<item type="tcl_script">ArubaOS_Config_Backup_Builder_Script.xml</item>
		</service>

	</services>
</adapter>

